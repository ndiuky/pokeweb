{
  "name": "pokeweb",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm exec vite",
    "build": "pnpm exec vue-tsc -b && pnpm exec vite build",
    "preview": "pnpm exec vite preview",
    "format": "pnpm exec biome format --write src/*",
    "lint": "pnpm exec biome lint src/*",
    "lint:fix": "pnpm exec biome check --write src/*"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "tailwindcss": "^4.1.17",
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vue-tsc": "^3.1.4"
  }
}
